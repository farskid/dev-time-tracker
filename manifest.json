{
  "name": "Dev Time Tracker",
  "version": "0.1.0",
  "description": "A simple time tracker to serve developers",
  "permissions": ["storage"],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "offline_enabled": true,
  "browser_action": {
    "default_title": "Dev Time Tracker",
    "default_icon": "icons/32.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "content_security_policy":
    "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
